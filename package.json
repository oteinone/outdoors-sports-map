{
  "name": "outdoors-sports-map-monorepo",
  "version": "2.6.0",
  "private": true,
  "description": "Outdoor Exercise Map - Full Stack Application",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "npm run start --workspace=frontend",
    "dev:backend": "npm run dev --workspace=backend",
    "start": "npm run start:backend",
    "start:frontend": "npm run start --workspace=frontend",
    "start:backend": "npm run start --workspace=backend",
    "build": "npm run build:frontend",
    "build:frontend": "npm run build --workspace=frontend",
    "build:backend": "echo 'Backend needs no build step'",
    "test": "npm run test:frontend",
    "test:frontend": "npm run test --workspace=frontend",
    "install:all": "npm install",
    "clean": "npm run clean:frontend && npm run clean:backend",
    "clean:frontend": "rm -rf frontend/node_modules frontend/build",
    "clean:backend": "rm -rf backend/node_modules",
    "production": "NODE_ENV=production npm run start:backend"
  },
  "workspaces": [
    "frontend",
    "backend"
  ],
  "repository": {
    "url": "https://github.com/oteinone/outdoors-sports-map.git",
    "type": "git"
  },
  "author": "Otto Teinonen <otto.teinonen@gmail.com>",
  "contributors": [
    "Jukka-Pekka Salo <jpsalo@zoho.com> (Original City of Helsinki version)",
    "Christoffer Niska (Original author)"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}